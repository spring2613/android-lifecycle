<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        android:versionCode="1"
        android:versionName="1.0"
        package="com.example.lifecyclelog"
        >

    <application
            android:allowBackup="true"
            android:icon="@drawable/ic_launcher"
            android:label="@string/app_name"
            android:theme="@style/AppTheme"
            >
        <activity
                android:name=".CompatAActivity"
                android:label="@string/app_name_compat"
                android:taskAffinity="com.scvngr.levelup.lifecycle.compat"
                >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity
                android:name=".AActivity"
                android:label="@string/app_name"
                android:taskAffinity="com.scvngr.levelup.lifecycle.native"
                >
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name=".BActivity"/>
        <activity android:name=".CompatBActivity"/>
    </application>

</manifest>
